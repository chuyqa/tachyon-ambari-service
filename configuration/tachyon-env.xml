<configuration>
  <property>
   <name>tachyon.log.dir</name>
   <description>Tachyon Log Directory</description>
   <value>/var/log/tachyon/</value>
  </property>

  <property>
    <name>tachyon.pid.dir</name>
    <description>Tachyon pid Directory</description>
    <value>/var/run/tachyon</value>
  </property>

  <property>
    <name>tachyon_user</name>
    <value>tachyon</value>
    <property-type>USER</property-type>
    <description>Tachyon Service Account User</description>
  </property>
</configuration>
